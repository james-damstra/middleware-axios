{
  "name": "middleware-axios",
  "version": "1.0.0",
  "description": "Axios with express/koa like middleware",
  "source": "src/index.js",
  "main": "dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "dist/index.module.js",
  "umd:main": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "peers": "npm-install-peers",
    "test": "jest",
    "test:coverage": "npm run peers && jest --coverage",
    "start:example": "webpack serve -c ./example/webpack.config.js",
    "build": "microbundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krutoo/middleware-axios.git"
  },
  "keywords": [
    "axios",
    "fetch",
    "api",
    "javascript",
    "middleware"
  ],
  "author": "Dmitry Petrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krutoo/middleware-axios/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:coverage"
    }
  },
  "homepage": "https://github.com/krutoo/middleware-axios#readme",
  "peerDependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-modules": "^0.1.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.17.0",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "npm-install-peers": "^1.2.1",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
